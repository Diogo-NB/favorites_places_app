{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths" : {
      "@components/*": ["./src/components/*"],
      "@context/*": ["./src/context/*"],
      "@models/*": ["./src/models/*"],
      "@navigation/*": ["./src/navigation/*"],
      "@screens/*": ["./src/screens/*"],
      "@services/*": ["./src/services/*"],
      "@theme/*": ["./src/theme/*"],
